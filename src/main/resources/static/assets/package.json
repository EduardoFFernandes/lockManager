{
  "name": "template_custom_colors",
  "version": "1.0.0",
  "description": "color template",
  "scripts": {
    "watch-sass": "node-sass --watch -r scss/ -o css/ --output-style=compressed",
    "compile-sass-app": "node-sass --source-map true -q scss/app.scss css/app.css --output-style compressed",
    "compile-sass-icons": "node-sass --source-map true -q scss/icons.scss css/icons.css --output-style compressed",
    "compile-sass-style": "node-sass --source-map true -q scss/style.scss css/style.css --output-style compressed",
    "build-sass": "npm-run-all compile-sass-app compile-sass-icons compile-sass-style"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "materialize-css": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5"
  }
}
